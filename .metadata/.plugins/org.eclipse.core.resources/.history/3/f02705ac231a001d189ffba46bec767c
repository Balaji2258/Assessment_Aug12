package com.assignment.studentPackage1;

public class GradePrediction extends Student{
	public void predict() {
		if(perc >= 90)
			grade = 'A';
		else if(perc >= 80)
			grade = 'B';
		else if(perc >= 70)
			grade = 'C';
		else if(perc >= 60)
			grade = 'D';
		else if(perc >= 50)
			grade = 'E';
		else
			grade = 'F';
	}
}
